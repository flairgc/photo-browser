{
  "name": "kinoclub",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "dependencies": {
    "axios": "^1.9.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "concurrently": "^8.0.0",
    "prettier": "^3.5.3"
  },
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm:dev:backend\" \"npm:dev:frontend\"",
    "dev:backend": "npm --workspace=backend run dev",
    "dev:frontend": "npm --workspace=frontend run dev",
    "db:reset": "npm run prepare:env && npm --workspace=backend run resetdb"
  }
}